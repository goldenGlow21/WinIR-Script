{
  "ResultDir": "./results",
  "ToolsPath": {
    "Sysinternals": "C:/tools/Sysinternals",
    "NirSoft": "C:/tools/NirSoft"
  },
  "Collection": {
    "EnableVolatile": true,
    "EnableNonVolatile": true,
    "IncludeRegistry": true,
    "IncludeEventLogs": true
  },
  "Compression": {
    "Enable": true,
    "Format": "zip"
  },
  "Hash": {
    "Algorithms": [ "MD5", "SHA256" ],
    "VerifyAfterCollection": true
  }
}
